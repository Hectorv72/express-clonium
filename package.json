{
  "name": "express-clonium",
  "version": "1.0.0",
  "description": "Servidor del juego clonium",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start-dist": "node dist/index.js",
    "deva": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "dev-test": "concurrently \" tsc -w \" \" node ./dist/src/index.js \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hectorv72/express-clonium.git"
  },
  "author": "Hector Valdez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hectorv72/express-clonium/issues"
  },
  "homepage": "https://github.com/Hectorv72/express-clonium#readme",
  "dependencies": {
    "express": "^4.17.3",
    "socket.io": "^4.4.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "morgan": "^1.10.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1"
  }
}
